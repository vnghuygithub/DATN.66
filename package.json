{
  "name": "react-antd-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.8.1",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.5.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@tinymce/tinymce-react": "^4.3.0",
    "antd": "^4.17.2",
    "array-to-tree": "^3.3.2",
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.3.11",
    "dotenv-cli": "^7.2.1",
    "driver.js": "^0.9.8",
    "he": "^1.2.0",
    "history": "^5.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.30.1",
    "qrcode.react": "^3.1.0",
    "query-string": "^7.0.1",
    "react": "^17.0.0",
    "react-big-calendar": "^1.14.1",
    "react-css-theme-switcher": "^0.3.0",
    "react-dom": "^17.0.0",
    "react-icons": "^5.2.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intl": "^5.23.0",
    "react-lazyload": "^3.2.0",
    "react-otp-input": "^3.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.0",
    "recharts": "^2.1.13",
    "sass": "^1.62.1",
    "xlsexport": "^1.5.2",
    "xlsx": "^0.18.5",
    "yarn": "^1.22.19"
  },
  "scripts": {
    "commit": "czg",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:stg": "tsc && vite build --mode=staging",
    "build:dev": "tsc && vite build --mode=dev",
    "build:prod": "tsc && vite build --mode=production",
    "serve": "vite preview",
    "lint": "eslint . --ext js,ts,tsx",
    "format": "prettier --write **/*.{js,ts,tsx} && eslint . --ext js,ts,tsx --fix",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/he": "^1.2.3",
    "@types/lodash": "^4.14.195",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^16.11.6",
    "@types/qrcode": "^1.5.0",
    "@types/react": "^17.0.0",
    "@types/react-big-calendar": "^1.8.12",
    "@types/react-dom": "^17.0.0",
    "@types/react-lazyload": "^3.2.0",
    "@types/react-redux": "^7.1.20",
    "@types/recharts": "^1.8.13",
    "@types/webpack-env": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-react": "^1.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.4.1",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-imp": "^2.0.10",
    "vite-plugin-svgr": "^0.6.0"
  },
  "resolutions": {
    "@types/react": "17.0.44"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
